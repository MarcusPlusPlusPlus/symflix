<p>{{ serie.name }}</p>
<p>{{ serie.description }}</p>
<p>{{ serie.creationDate | date('d-m-Y') }}</p>
<p>{{ serie.releaseDate | date('d-m-Y') }}</p>
<P>{{ serie.durationTime }}</P>

{% for episode in serie.episodes %}
    <p>{{ episode.description }}</p>
    <p>{{ episode.name }}</p>
    <video>{{ episode.video }}</video>
    <p>{{ episode.durationTime }}</p>
    
{% endfor %}



